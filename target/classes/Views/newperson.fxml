<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="438.0" prefWidth="429.0" styleClass="background" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CreationController">
   <children>
      <Label layoutX="100.0" layoutY="21.0" prefHeight="48.0" prefWidth="257.0" text="Person creation">
         <font>
            <Font name="Lobster" size="37.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" layoutX="70.0" layoutY="327.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="104.0" styleClass="table__button" stylesheets="@styles.css" text="Save" />
      <Button fx:id="cancelButton" layoutX="255.0" layoutY="327.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="104.0" style="-fx-text-fill: white; -fx-background-color: #141846; -fx-border-radius: 14px; -fx-background-radius: 14px;" stylesheets="@styles.css" text="Cancel" />
      <DatePicker fx:id="birthdayField" layoutX="122.0" layoutY="165.0" prefHeight="37.0" prefWidth="185.0" promptText="01.01.1980" />
      <TextField fx:id="nameField" layoutX="100.0" layoutY="94.0" prefHeight="37.0" prefWidth="229.0" promptText="Bill Gates">
         <font>
            <Font name="Lato Black" size="19.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
